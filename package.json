{
  "name": "howufeel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env vite",
    "dev:emulator": "dotenv -e .env.development -- vite",
    "emulators": "firebase emulators:start",
    "dev:full": "concurrently \"npm run emulators\" \"npm run dev:emulator\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:rules": "firebase emulators:exec --only firestore,auth 'dotenv -e .env.test -- jest tests/security-rules/index.test.ts'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/playfair-display": "5.2.5",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-charts": "7.28.0",
    "@mui/x-date-pickers": "^7.28.3",
    "@tailwindcss/vite": "^4.1.3",
    "@types/canvas-confetti": "1.9.0",
    "canvas-confetti": "1.9.3",
    "dayjs": "^1.11.13",
    "firebase": "11.6.0",
    "framer-motion": "12.6.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-infinite-scroll-component": "6.1.0",
    "react-intersection-observer": "9.16.0",
    "react-router": "^7.5.0",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@firebase/rules-unit-testing": "4.0.1",
    "@types/jest": "29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "9.1.2",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "firebase-tools": "^13.5.0",
    "globals": "^15.15.0",
    "jest": "29.7.0",
    "terser": "5.39.0",
    "ts-jest": "29.3.2",
    "tsx": "4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
